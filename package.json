{
  "name": "scentgraph",
  "version": "1.0.0",
  "description": "ScentGraph is a project for Helsinki University FullStack course. The plan is to use React and TypeScript for the front end, Node for the backend and Neo4J for graph database. The database queries will be done using Cypher.",
  "main": "index.js",
  "scripts": {
    "start-server": "ts-node -r dotenv/config --project ./config/tsconfig-server.json ./src/server/index.ts",
    "test": "NODE_ENV=test jest --verbose --runInBand",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apndx/ScentGraph.git"
  },
  "author": "apndx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apndx/ScentGraph/issues"
  },
  "homepage": "https://github.com/apndx/ScentGraph#readme",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "neo4j-driver": "^4.0.1",
    "neode": "^0.3.7",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.0",
