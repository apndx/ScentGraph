    "@types/jest": "^25.1.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/request-promise": "^4.1.44",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "neo4j-driver": "^4.0.1",
    "neode": "^0.3.7",
    "parcel-bundler": "^1.12.4",
